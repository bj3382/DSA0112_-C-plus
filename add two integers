#include <iostream>

class Add {
public:
    int add(int a, int b) {
        return a + b;
    }

    float add(float a, float b) {
        return a + b;
    }
};

int main() {
    Add obj;
    std::cout << "Sum of integers: " << obj.add(5, 3) << std::endl;
    std::cout << "Sum of floats: " << obj.add(5.5f, 3.2f) << std::endl;
    return 0;
}