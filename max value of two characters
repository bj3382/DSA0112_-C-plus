#include <iostream>

class MaxFinder {
public:
    int max(int a, int b) {
        return (a > b) ? a : b;
    }

    float max(float a, float b) {
        return (a > b) ? a : b;
    }

    char max(char a, char b) {
        return (a > b) ? a : b;
    }
};

int main() {
    MaxFinder obj;
    std::cout << "Max of 5 and 10: " << obj.max(5, 10) << std::endl;
    std::cout << "Max of 5.5 and 3.2: " << obj.max(5.5f, 3.2f) << std::endl;
    std::cout << "Max of 'a' and 'z': " << obj.max('a', 'z') << std::endl;
    return 0;
}